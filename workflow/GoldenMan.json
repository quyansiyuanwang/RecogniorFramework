{
  "$schema": "./schema/generated.schema.json",
  "begin": "CheckButton",
  "globals": {
    "debug": true,
    "logConfig": {
      "file": "log/test.log"
    }
  },
  "jobs": {
    "CheckButton": {
      "type": "ROI",
      "image": {
        "path": "images/sky_auto/goldman_button.jpg",
        "confidence": 0.7
      },
      "next": { "success": "GetGoldenMan", "failure": "CheckButton" }
    },
    "GetGoldenMan": {
      "type": "Input",
      "action": {
        "type": "KeyboardInput",
        "keys": ["f"]
      },
      "next": "CheckButton",
      "delay": {
        "cur": 50,
        "post": 5000
      }
    }
  }
}
